<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>RoomListServlet</servlet-name>
    <servlet-class>endes.RoomListServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RoomListServlet</servlet-name>
    <url-pattern>/rooms</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>RoomServlet</servlet-name>
    <servlet-class>endes.RoomServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RoomServlet</servlet-name>
    <url-pattern>/room</url-pattern>
  </servlet-mapping>
</web-app>
